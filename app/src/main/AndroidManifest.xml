<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.proyect.moodle">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".listado_clases"></activity>
        <activity android:name=".crear_horario" />
        <activity android:name=".docente_gestiona" />
        <activity android:name=".listar_asignaturas" />
        <activity android:name=".crear_asignatura" />
        <activity android:name=".listado_docentes" />
        <activity android:name=".crear_docente" />
        <activity android:name=".decano_gestion" />
        <activity android:name=".clase_gestion" />
        <activity android:name=".clase_activa" />
        <activity android:name=".vista_semestre" />
        <activity android:name=".vista_semana" />
        <activity android:name=".vista_dia" />
        <activity android:name=".confirmar_gps" />
        <activity android:name=".verificar_gps" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>